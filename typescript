#!/bin/bash


# Simple script to test echo commands

echo "This is a test script."
echo "It prints multiple lines."
echo "Each line is printed by an echo command."
echo "This helps in testing the script execution functionality."
echo "If you see all these lines, the script executed successfully."
echo "If you see all these lines, the script executed successfully."
# End of script

